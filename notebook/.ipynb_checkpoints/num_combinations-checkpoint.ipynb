     "input": [
      "def comb(N, k):\n",
      "    if k <= 1:\n",
      "        return [[r] for r in range(1, N+1)]\n",
      "    if N <= k:\n",
      "        return [range(1, N+1)]\n",
      "    return comb(N-1, k) + [s+[N] for s in comb(N-1, k-1)]"
     ],
     "language": "python",
